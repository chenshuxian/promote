generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model apply {
  id           String    @id @default("null") @db.VarChar(45)
  parent_id    String?   @db.VarChar(45)
  house_id     String    @default("0") @db.VarChar(45)
  born         Int       @default(0)
  bank_id      Int       @default(700)
  bank_account String    @default("0") @db.VarChar(45)
  status       Int       @default(0)
  is_delete    Int       @default(0)
  create_time  DateTime  @default(now()) @db.Timestamp(0)
  update_time  DateTime? @db.Timestamp(0)
  ip           Int?      @db.UnsignedInt
}

model users {
  id          Int       @id @default(autoincrement())
  name        String    @db.VarChar(45)
  password    String    @default("12345678") @db.VarChar(45)
  create_time DateTime  @default(now()) @db.Timestamp(0)
  update_time DateTime? @db.Timestamp(0)
  is_delete   Int       @default(0)
}
